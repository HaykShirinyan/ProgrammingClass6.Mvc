@model List<ProductType>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<div>
   <table class="table"
       <thead>
           <tr>
               <th>
                   Id
               </th>
               <th>
                   Title
               </th>
               <th>
                   Brand
               </th>
               <th>
                   Type
               </th>
               <th>
                     Price
               </th>
           </tr>
       </thead>

       <tbody>
           @foreach (ProductType productType in Model)
           {
               <tr>
                   <td>
                       @productType.Id
                   </td>
                   <td>
                       @productType.Title
                   </td>
                   <td>
                       @productType.Brand
                   </td>
                   <td>
                       @productType.Type
                   </td>
                   <td>
                       @productType.Price
                   </td>
               </tr>
           }
       </tbody>
   </table>
</div>
