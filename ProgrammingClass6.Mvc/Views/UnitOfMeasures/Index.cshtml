@model List<UnitOfMeasure>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    ID
                </th>
                <th>
                    Unit
                </th>                
            </tr>
        </thead>

        <tbody>
            @foreach (UnitOfMeasure unitOfMeasure in Model)
            {
                <tr>
                    <td>
                        @unitOfMeasure.Id
                    </td>
                    <td>
                        @unitOfMeasure.Unit
                    </td>                    
                </tr>
            }
        </tbody>
    </table>
</div>
