@model List<UnitOfMeasure>


@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<div>
    <table class="table"
        <thead>
            <tr
                <th>
                        ID
                </th>
                <th>
                        UnitName
                </th
                <th>
                        UnitShortName
                </th>
                <th>
                        Diametere
                 </th>

            </tr>
        </thead>

        <tbody>
            @foreach (UnitOfMeasure unitOfMeasure in Model)
            {
                    <tr>
                        <td>
                            @unitOfMeasure.Id
                        </td>
                        <td>
                            @unitOfMeasure.UnitName
                        </td>
                        <td>
                            @unitOfMeasure.UnitShortName
                        </td>
                        <td>
                            @unitOfMeasure.Diametere
                        </td>
                    </tr>
            }
)
        </tbody>

    </table>
</div>
