@{
}


<h1 class="h3">
    Create Name
</h1>

<form asp-controller="ProductTypes" asp-action="Create">
    <div class="mb-3">
        <label asp-for="Name" class="form-label">
            Unit
        </label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>

    <div>
        <a class="btn btn-secondary" asp-controller="ProductTypes" asp-action="Index">
            Cancel
        </a>

        <button class="btn btn-primary" type="submit">
            Save
        </button>
    </div>
</form>

