@model List<Product>

@{

}

<div>
    <h1> List </h1>
    <table class="table">
        <thead>
            <tr>
                <th>
                    ID
                </th>
                <th>
                    Name
                </th>
                <th>
                    Brand
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Unit Price
                </th>
                <th>
                    In Stock
                </th>
            </tr>
        </thead>

        <tbody>
            @foreach (Product product in Model)
            {
                <tr>
                    <td>
                        @product.Id
                    </td>
                    <td>
                        @product.ProductName
                    </td>
                    <td>
                        @product.Brand
                    </td>
                    <td>
                        @product.Quantity
                    </td>
                    <td>
                        @product.Price
                    </td>
                    <td>
                        @product.InStock
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
